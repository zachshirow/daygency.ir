---
import Link from '@/components/Link.astro'
import { Image } from 'astro:assets'
import logo from '../../public/static/logo.svg'
import { NAV_LINKS, SITE } from '@/consts'
---


<Link
href="/"
class="flex flex-shrink-0 items-center gap-2 text-xl font-semibold transition-colors duration-300 hover:text-primary no-underline justify-center"
>
<Image src={logo} alt="Logo" class="size-8 dark:invert" />
{SITE.TITLE}
</Link>
